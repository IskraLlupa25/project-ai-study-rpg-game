{
  "version": 2,
  "builds": [
    {
      "src": "Canvas_Game/src/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "Canvas_Game/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Canvas_Game/src/app.py"
    }
  ],
  "framework": "vite"
}
